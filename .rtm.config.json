{
  "dateformat": "ddd mmm-dd",
  "plannerDateformat": "ddd",
  "completed": 7,
  "hideDue": false,
  "plain": false,
  "status": true,
  "styles": {
    "list": "yellow.underline",
    "index": "dim",
    "priority": {
      "0": "reset",
      "1": "red",
      "2": "blue",
      "3": "cyan"
    },
    "completed": "dim",
    "notes": "reset",
    "tags": "magenta",
    "due": "green"
  },
    "aliases": [
    {
      "name": "important",
      "description": "Display incomplete tasks with a priority of 1",
      "command": "ls",
      "args": "priority:1 AND status:incomplete"
    },
{
      "name": "work",
      "description": "Display tasks related to work alone",
      "command": "ls",
      "args": "list:inbox AND NOT tag:personal"
    },
{
      "name": "personal",
      "description": "Display personal tasks alone",
      "command": "ls",
      "args": "list:secondary"
    },
{
      "name": "special",
      "description": "Display tasks from the special list",
      "command": "ls",
      "args": "list:special"
    },
{
      "name": "inbox",
      "description": "Display tasks from the list inbox",
      "command": "ls",
      "args": "list:inbox "
    }
  ],
  "plugins": [
    "{{HOME}}/.rtm.plugins/rtm-cli-plugin-example"
  ]
}

